<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="account_journal_boleta" model="account.journal">
            <field name="name">Boleta</field>
            <field name="code">BOL</field>
            <field name="type">sale</field>
            <!-- avoid being selected as default journal -->
            <field name="sequence">99</field>
            <field name="currency_id" ref="base.CLP"/>
            <field name="l10n_latam_use_documents" eval="True"/>
            <field name="l10n_cl_point_of_sale_type">online</field>
            <field name="l10n_latam_document_type_id" ref="l10n_cl.dc_b_f_dte"/>
        </record>

    </data>
</odoo>